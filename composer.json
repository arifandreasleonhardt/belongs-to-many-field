{
    "name": "benjacho/belongs-to-many-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],	
    "autoload": {
        "psr-4": {
            "Benjacho\\BelongsToManyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benjacho\\BelongsToManyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
